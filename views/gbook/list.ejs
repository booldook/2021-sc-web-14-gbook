<tr>
	<td><%= v.id %></td>
	<td><%= v.writer %> </td>
	<td class="text-left"><%= v.content %> </td>
	<td>
		<% if(v.savename) { %> 
			<img src="<%= v.savename %> " alt="첨부이미지" onclick="onImgModal(this);">
		<% } %>
	</td>
	<td><%= v.createdAt %> </td>
	<td>
		<button onclick="onUpdate(this);" class="btn btn-sm btn-success">수정</button>
		<button onclick="onDelete(this);" class="btn btn-sm btn-danger">삭제</button>
	</td>
</tr>
<script src="/js/gbook/list.js"></script>